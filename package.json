{
  "name": "rapido",
  "private": true,
  "scripts": {
    "dev": "nodemon app.js",
    "build": "nuxt build",
    "start": "node app.js",
    "generate": "nuxt generate",
    "eslint": "eslint --fix --ext .js,.vue .",
    "stylelint": "stylelint ./**/*.{vue,scss} --syntax scss --ignore-path .gitignore",
    "lint": "yarn run eslint && yarn run stylelint",
    "test:unit": "NODE_ENV=test jest --config=jest.config.js --maxWorkers=2",
    "test:e2e": "start-server-and-test start http://localhost:4000 cypress:run",
    "cypress:run": "cypress run",
    "cypress": "cypress open",
    "cypress:verify": "cypress verify",
    "dev-debug": "NODE_ENV=development API_BROWSER='https://api-hub.eu-west-1.production.cgaws.cloud' API_SERVER='https://api-hub.eu-west-1.production.cgaws.cloud' node --inspect node_modules/.bin/nuxt --port 4000",
    "dev-debug:acc": "NODE_ENV=development API_BROWSER='https://api-hub.eu-west-1.acceptance.cgaws.cloud' API_SERVER='https://api-hub.eu-west-1.acceptance.cgaws.cloud' node --inspect node_modules/.bin/nuxt --port 4000",
    "dev-debug:beltegoed": "LABEL=beltegoed NODE_ENV=development API_BROWSER='https://api-hub.eu-west-1.production.cgaws.cloud' API_SERVER='https://api-hub.eu-west-1.production.cgaws.cloud' node --inspect node_modules/.bin/nuxt --port 4000",
    "dev-debug:phraseapp": "PHRASEAPP_EDITOR=enabled NODE_ENV=development API_BROWSER='https://api-hub.eu-west-1.production.cgaws.cloud' API_SERVER='https://api-hub.eu-west-1.production.cgaws.cloud' node --inspect node_modules/.bin/nuxt --port 4000",
    "storybook": "LABEL=beltegoed start-storybook -p 6060",
    "storybook:build": "LABEL=beltegoed build-storybook -c .storybook -o docs/storybook",
    "storybook:deploy": "storybook-to-ghpages",
    "storybook:snapshots": "NODE_ENV=test jest --config=.storybook/jest.config.js --maxWorkers=2",
    "storybook:update-snapshots": "NODE_ENV=test jest --updateSnapshot --config=.storybook/jest.config.js --maxWorkers=2"
  },
  "nodemonConfig": {
    "legacyWatch": true,
    "watch": [
      "app.js",
      "nuxt-config.js",
      "config/i18nConfig.js"
    ]
  },
  "engines": {
    "node": "<= 10"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@nuxtjs/sentry": "^3.0.0",
